using System;

namespace Atividade_6_lista_8
{
    class Program
    {
        public static void Main()
        {
        	int mult = 0;
        	
        	for(int i = 1; i <= 10; i++)
        	{
        		mult = i * 5;
        		Console.WriteLine(mult);
        	}
            
            Console.WriteLine("Press any key to continue . . . ");
            Console.ReadKey();
        }
    }
}
