using System;

namespace Atividade_6_lista_8
{
    class Program
    {
        public static void Main()
        {

         Console.WriteLine("Insira um número inteiro positivo:");
         int num = int.Parse(Console.ReadLine());
         Console.WriteLine("Os divisores de " + num + " são:");
  
            for (int i = 1; i <= num; i++)
            {
                if (num % i == 0)  
                {
                    Console.WriteLine(i);  
                }
            }

    
      
            Console.WriteLine("");
            Console.WriteLine("Press any key to continue . . . ");
            Console.ReadKey();
        }
    }
}
